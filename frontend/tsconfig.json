{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,

    "baseUrl": "src",
    "paths": { "@/*": ["./*"] },

    "types": ["vite/client"]    
  },
  "include": ["src"]            
}